<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_pc_reports" model="ir.ui.view">
        <field name="name">res.company.form.inherit.pc.reports</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="before">
                <field name="pc_local_address"/>
                <field name="pc_show_packing_list_as_do"/>
                <field name="pc_company_stamp" widget="image"/>
            </xpath>
        </field>
    </record>
</odoo>
